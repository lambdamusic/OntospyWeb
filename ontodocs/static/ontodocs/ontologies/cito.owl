        <rdfs:subPropertyOf rdf:resource="http://purl.org/spar/cito/isCitedBy"/>
        <owl:inverseOf rdf:resource="http://purl.org/spar/cito/qualifies"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/cito/isRefutedBy -->

